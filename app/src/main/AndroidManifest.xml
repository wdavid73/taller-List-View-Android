<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tallerlistview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Volumes.Cube"></activity>
        <activity android:name=".Volumes.Cone" />
        <activity android:name=".Volumes.Cylinder" />
        <activity android:name=".Volumes.Sphere" />
        <activity android:name=".Areas.Circle" />
        <activity android:name=".Areas.Triangle" />
        <activity android:name=".Areas.Rectangle" />
        <activity android:name=".ListOperations" />
        <activity android:name=".Areas.Square" />
        <activity android:name=".CalculateVolumes" />
        <activity android:name=".CalculateAreas" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>